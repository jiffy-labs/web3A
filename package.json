{
  "name": "@jiffy-labs/web3a",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "version": "0.1.4",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
