    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.11.19",
    "bun-types": "latest",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@open-rpc/client-js": "^1.8.1",
    "dotenv": "^16.4.1",
    "fs": "^0.0.1-security",
    "json-rpc-2.0": "^1.7.0",
    "permissionless": "^0.1.18",
    "viem": "^2.9.21"
  }
}
